package models

type Todo struct {
	Id        int       `json:"id"`
	Name      string    `json:"name"`
	ExchangeRate int	`json:"exchangeRate"`  // base currency is US dollar
	Updated int `json:"updated"`
	Created	int	`json:"created"`
}

